<Window x:Class="Biblioteka2cas.DodajAutoraProzor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Dodaj Autora" Height="200" Width="300">
    <Grid>
        <StackPanel>
          

            <Label Content="Unesi ime" Margin="50, 0, 50, 0"/>
            <TextBox Name="txtIme" Margin="50, 0, 50, 0"/>
            <Label Content="Unesi prezime" Margin="50, 0, 50, 0"/>
            <TextBox Name="txtPrezime" Margin="50, 0, 50, 0"/>
            
            <Button Content="Dodaj" Click="btnDodaj_Click" Margin="50, 50, 50, 50"/>
        </StackPanel>
    </Grid>
</Window>